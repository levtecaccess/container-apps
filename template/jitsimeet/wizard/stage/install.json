{
  "api_version": "v1",
  "title": "{{JITSIMEET_NAME}}",
  "wizard": [
    {
      "title": "{{JITSIMEET_BASE_PAGE}}",
      "schema": {
        "http_port": {
          "title": "HTTP Port",
          "type": "integer",
          "description": "{{JITSIMEET_HTTP_PORT_DESC}}",
          "minimum": 1,
          "maximum": 65535,
          "required": true
        },
        "https_port": {
          "title": "HTTPS Port",
          "type": "integer",
          "description": "{{JITSIMEET_HTTPS_PORT_DESC}}",
          "minimum": 1,
          "maximum": 65535,
          "required": true
        }
      },
      "form": [
        "*"
      ]
    }
  ],
  "binding": {
    "type": "yaml",
    "file": "docker-compose.yml",
    "data": {
      "http_port": [
        "services.web.ports[0]",
        "services.web.environment.HTTP_PORT"
      ],
      "https_port": [
        "services.web.ports[1]",
        "services.web.environment.HTTPS_PORT"
      ]
  }
}
